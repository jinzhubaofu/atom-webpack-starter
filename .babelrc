{
    "env": {
        "dev": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "browsers": ["chrome >= 60"]
                        },
                        "modules": "commonjs",
                        "useBuiltIns": true
                    }
                ]
            ],
            "plugins": [
                "syntax-dynamic-import",
                "dual-import"
            ]
        },
        "production": {
            "presets": ["env"],
            "plugins": [
                "transform-runtime",
                "syntax-dynamic-import",
                "dual-import"
            ]
        }
    }
}
